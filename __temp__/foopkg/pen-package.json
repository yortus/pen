{
    "name": "foopkg",


    // If "export" resolves to a file (ie src/interface.pen):
    // - only the exported decls in this one file are exported. No namespaces are exported. ie it's a 'leaf'

    // If "export" resolved to a directory (ie src/interface/index.pen):
    // - everything in the directory is searched for exports, including nested namespaces (subdirs) and their modules
    "export": "src/interface"
}
