    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist/",
        "build": "yarn build:cjs && yarn build:tests",
        "build TODO restore this!": "yarn build:cjs && yarn build:umd && yarn build:tests",
        "build:cjs": "tsc -p src && pegjs -o dist/commonjs/grammar.js src/grammar.pegjs",
        "build:umd": "webpack",
        "build:tests": "tsc -p test && cpx test/fixtures/*.* dist/test/fixtures",
        "lint": "tslint src/**/*.ts test/**/*.ts",
        "test": "node ./scripts/run-unit-tests",
        "test:debug": "cross-env DEBUG=multimethods node ./scripts/run-unit-tests",
        "bench": "node ./dist/extras/bench/basic-routing.js",
        "bench:debug": "cross-env DEBUG=multimethods node ./dist/extras/bench/basic-routing.js",
        "prepare": "npm run self-ref && npm run build",
        "self-ref": "node ./scripts/enable-self-reference",
        "test:bc TODO restore this!": "node --print-bytecode >bytecode.txt packages/usage-tests/math/dist/index.js"
    },
